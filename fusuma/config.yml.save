swipe:
  3:
    left:
      command: "xdotool key Ctrl+Super+Right" # History forward 
    right:
      command: "xdotool key Ctrl+Super+Left" # History back
    up:
      command: "xdotool key Super+A" # Activity
    down:
      command: "xdotool key Super" # Activity
  4:
    left:
      command: "xdotool key Ctrl+Super+Down" # Switch to next workspace
    right:
      command: "xdotool key Ctrl+Super+Up" # Switch to previous workspace
    up:
      command: "xdotool key Super+M" # Switch to next workspace
    down:
      command: "xdotool key  # Switch to previous workspace
pinch:
  in:
    command: "xdotool keydown ctrl click 4 keyup ctrl" # Zoom in
  out:
    command: "xdotool keydown ctrl click 5 keyup ctrl" # Zoom out
